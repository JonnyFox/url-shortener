{
    "name": "url-shortener",
    "version": "1.0.0",
    "description": "",
    "main": "dist/server.js",
    "scripts": {
        "clean": "rimraf dist",
        "start": "node ./dist/server.js",
        "dev": "concurrently \"npm run nodemon\" \"npm run build.w\"",
        "prebuild": "npm run clean",
        "build": "concurrently \"npm run build.js\" \"npm run build.html\"",
        "build.js": "tsc",
        "build.html": "copy-newer --cwd src ./**/*.html ./dist",
        "build.w": "tsc -w",
        "nodemon": "SET NODE_ENV=dev && nodemon --watch dist dist/server.js",
        "postinstall": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JonnyFox/url-shortener.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/JonnyFox/url-shortener/issues"
    },
    "homepage": "https://github.com/JonnyFox/url-shortener#readme",
    "dependencies": {
        "@types/express": "^4.0.33",
        "@types/mongodb": "^2.1.33",
        "@types/node": "^6.0.46",
        "concurrently": "^3.1.0",
        "copy-newer": "^2.1.2",
        "express": "^4.14.0",
        "inversify": "^3.0.0-beta.1",
        "mongodb": "^2.2.11",
        "reflect-metadata": "^0.1.8",
        "rimraf": "^2.5.4",
        "typescript": "^2.0.7"
    },
    "devDependencies": {
        "nodemon": "^1.11.0"
    }
}